{
  "version": 2,
  "builds": [
    {
      "src": "feedback-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/feedback-server.js"
    },
    {
      "src": "/feedback/(.*)",
      "dest": "/public/feedback.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
