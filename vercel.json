{
  "version": 2,
  "builds": [
    {
      "src": "feedback-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/feedback-server.js"
    },
    {
      "src": "/feedback/(.*)",
      "dest": "/public/feedback.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "feedback-server.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/feedback/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
